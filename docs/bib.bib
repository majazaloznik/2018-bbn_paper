keywords = {Social-ecological modeling,complex systems,ecological structure,human-decision making,social structure,uncertainty},
number = {1},
pages = {219--272},
publisher = {Blackwell Publishing Inc},
title = {{New horizons for managing the environment: A review of coupled social-ecological systems modeling}},
url = {http://dx.doi.org/10.1111/j.1939-7445.2011.00108.x},
volume = {25},

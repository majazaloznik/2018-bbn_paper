issn = {13648152},
journal = {Environmental Modelling {\&} Software},
month = {oct},
pages = {37--48},
title = {{Describing human decisions in agent-based models - ODD plus D, an extension of the ODD protocol}},
url = {http://www.sciencedirect.com/science/article/pii/S1364815213001394},
